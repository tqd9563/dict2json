{
  "name": "Dict to JSON",
  "displayName": "Dict to JSON",
  "description": "Convert Python dictionaries to standard JSON format with auto-formatting",
  "publisher": "yuiko",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.105.0"
  },
  "icon": "resource/icon.png",
  "categories": [
    "Formatters",
    "Other"
  ],
  "activationEvents": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/tqd9563/dict2json"
  },
  "license": "MIT",
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "dict2json.convert",
        "title": "Dict to JSON: Convert Python Dict to JSON"
      },
      {
        "command": "dict2json.formatJson",
        "title": "Dict to JSON: Format JSON"
      },
      {
        "command": "dict2json.minifyJson",
        "title": "Dict to JSON: Minify JSON"
      }
    ],
    "keybindings": [
      {
        "command": "dict2json.convert",
        "key": "ctrl+alt+j",
        "mac": "cmd+alt+j",
        "when": "editorTextFocus"
      },
      {
        "command": "dict2json.formatJson",
        "key": "ctrl+alt+f",
        "mac": "cmd+alt+f",
        "when": "editorTextFocus"
      },
      {
        "command": "dict2json.minifyJson",
        "key": "ctrl+alt+m",
        "mac": "cmd+alt+m",
        "when": "editorTextFocus"
      }
    ],
    "configuration": {
      "title": "Dict to JSON",
      "properties": {
        "dict2json.indentSize": {
          "type": "number",
          "default": 4,
          "minimum": 1,
          "maximum": 16,
          "description": "Number of spaces for JSON indentation (default: 4)"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.105.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "eslint": "^9.16.0",
    "typescript": "^5.7.2",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
